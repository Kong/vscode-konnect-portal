{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "outDir": "src/webview",
    "target": "ES2020",
    "module": "ES2020",
    "noEmitOnError": true,
    "allowJs": false,
    "checkJs": false,
    "removeComments": true,
    "sourceMap": false,
    "strict": true
  },
  "include": [
    "src/webview/webview.ts"
  ],
  "exclude": [
    "node_modules",
    "src/webview/webview.js"
  ]
}
